{
  "manifest_version": 3,
  "name": "Notion Highlighter",
  "version": "1.0",
  "description": "Enhances Notion page highlights.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.notion.so/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}